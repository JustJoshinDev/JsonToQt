{
  "title": "Complex Form Example",
  "properties": {
    "intro_label": {
      "widget": "label",
      "text": "Welcome to the complex form!"
    },
    "user_info": {
      "widget": "group",
      "title": "User Information",
      "properties": {
        "name": {
          "widget": "lineedit",
          "title": "Full Name"
        },
        "age": {
          "widget": "spinbox",
          "title": "Age",
          "minimum": 0,
          "maximum": 120
        },
        "gender": {
          "widget": "radio",
          "title": "Gender",
          "enum": ["Male", "Female", "Other"]
        }
      }
    },
    "preferences": {
      "widget": "group",
      "title": "Preferences",
      "properties": {
        "subscribe": {
          "widget": "checkbox",
          "title": "Subscribe to newsletter"
        },
        "favorite_color": {
          "widget": "combobox",
          "title": "Favorite Color",
          "enum": ["Red", "Green", "Blue", "Yellow"]
        },
        "bio": {
          "widget": "textarea",
          "title": "Short Bio"
        }
      }
    },
    "settings": {
      "widget": "group",
      "title": "Settings",
      "properties": {
        "volume": {
          "widget": "doublespinbox",
          "title": "Volume",
          "minimum": 0.0,
          "maximum": 1.0,
          "step": 0.01
        },
        "notifications": {
          "widget": "toggle",
          "title": "Enable Notifications",
          "children": {
            "email_alert": {
              "widget": "checkbox",
              "title": "Email Alerts"
            },
            "sms_alert": {
              "widget": "checkbox",
              "title": "SMS Alerts"
            }
          }
        }
      }
    },
    "dynamic_sections": {
      "widget": "multi_toggle",
      "title": "Add Dynamic Sections",
      "enum": ["Section A", "Section B"],
      "children_map": {
        "Section A": {
          "properties": {
            "field1": {
              "widget": "lineedit",
              "title": "Field 1"
            },
            "field2": {
              "widget": "checkbox",
              "title": "Field 2"
            }
          }
        },
        "Section B": {
          "properties": {
            "field3": {
              "widget": "spinbox",
              "title": "Field 3",
              "minimum": 10,
              "maximum": 50
            },
            "field4": {
              "widget": "textarea",
              "title": "Field 4"
            }
          }
        }
      }
    },
    "submit_button": {
      "widget": "button",
      "text": "Submit"
    }
  }
}
